<!DOCTYPE html>
  <html>
    <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Mehrceey's Typing Space</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Google Webfont -->
    <link href='https://fonts.googleapis.com/css?family=PT+Mono' rel='stylesheet' type='text/css'>
    <!-- Bootstrap -->
    <link rel="stylesheet" href="css/bootstrap.css">
    <!-- Theme Style -->
    <link rel="stylesheet" href="css/style.css">
    <!--favicon-->
    <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="images/favicon.ico" type="image/x-icon">
  </head>
  <body>
    <!-- Header -->
    <header id="fh5co-header" role="banner">
      <div class="container">
        <!-- Logo -->
        <div id="fh5co-logo">
          <a href="/"><img src="images/mlogo1.jpg" width="100px" height="100px" alt="Work Logo"></a>
        </div>
        <!-- Logo -->
        <!-- Main Nav -->
        <div id="fh5co-main-nav">
          <nav id="fh5co-nav" role="navigation">
          </nav>
        </div>
        <!-- Main Nav -->
      </div>
    </header>
    <!-- Header -->
    <main role="main">
    <div class="container">
      <!-- Container for the demo -->
      <div class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-cell--12-col-tablet mdl-cell--12-col-desktop">
        <div class="mdl-card__supporting-text mdl-color-text--grey-600">
          <h3>Enter email and password below to sign into an existing account</h3>
          <h4>Enter email, username and password sign to create an account</h4>
          <input type="text" id="username" name="username" placeholder="Username"/><br /><br />
          <input type="text" id="email" name="email" placeholder="Email"/><br /><br />
          <input type="password" id="password" name="password" placeholder="Password"/><br/><br/>
          <button class="mdl-button mdl-js-button mdl-button--raised btn btn-info" id="quickstart-sign-in" name="signin">Sign In</button>
           <button class="btn btn-info" id="quickstart-sign-up"  name="signup">Sign Up</button>
          <button class="btn btn-info hide" disabled id="quickstart-verify-email" name="verify-email">Send Email Verification</button>
          <button class="btn btn-info hide" id="quickstart-password-reset" name="verify-email">Send Password Reset Email</button>
          <br><br><p id="create"><a href="#">Create an account</a></p><br><br>
        </div>
      </div>
    <!-- scripts -->  
    <!--Firebase Initialization-->
    <script src="https://www.gstatic.com/firebasejs/3.6.10/firebase.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script>
    // Initialize Firebase
    var config = {
      apiKey: "AIzaSyDTtXPIG5QLuOpEou_03cW9bwWlJsT55cU",
      authDomain: "typing-speed-app-c0ad7.firebaseapp.com",
      databaseURL: "https://typing-speed-app-c0ad7.firebaseio.com",
      storageBucket: "typing-speed-app-c0ad7.appspot.com",
      messagingSenderId: "461044281575"
    };
    firebase.initializeApp(config);
    </script>
    <script type="text/javascript">
      $("#username").hide();
      $("h4").hide();
      $("#quickstart-sign-up").hide();
      $("#create").on('click', function() {
        $("#quickstart-sign-in").hide();
        $("#username").show();
        $("h4").show();
        $("h3").hide();
        $("#quickstart-sign-up").show();
        $("#create").hide();
      });
    </script>
    <script src="js/app.js"></script>
    </main>
    <footer id="fh5co-footer" role="contentinfo">
      <div class="container">
        <div class="row">
          <ul class="fh5co-footer-social">
            <li><a href="http://twitter.com/mehrceey04">Twitter</a></li>
            <li><a href="http://facebook.com/OluwaMehrceey">Facebook</a></li>
            <li><a href="http://github.com/mehrceey04">Github</a></li>
            <li><a href="http://instagram.com/mehrceey04">Instagram</a></li>
          </ul>
          <p class="fh5co-copyright">(c) 2017 <a href="/">Home</a>. All Rights Reserved.<br></p>
        </div>
      </div>
    </div>
    </footer>
  </body>
</html>